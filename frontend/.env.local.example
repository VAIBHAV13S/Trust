# OneChain Configuration
# Copy this file to .env.local and fill in your values

# Your deployed game_manager contract package ID on OneChain
VITE_ONECHAIN_PACKAGE_ID=0x0

# OneChain RPC Endpoint (default: testnet)
VITE_ONECHAIN_RPC=https://rpc-testnet.onelabs.cc:443

# OneChain Chain ID (4002=testnet, 1088=mainnet)
VITE_ONECHAIN_CHAIN_ID=4002

# Gas budget for transactions (in ONE)
VITE_ONECHAIN_GAS_BUDGET=10000000
